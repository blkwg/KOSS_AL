import sys
given_list = sys.stdin.readline()
blast_list = sys.stdin.readline()

gll = len(given_list)
bll = len(blast_list)

def find_mine(result_list, length):
    break_check = 0
    for i in range(length - bll + 1):
        print(result_list[i:i + bll - 1], blast_list)
        if blast_list == result_list[i:i + bll - 1]:
            result_list = result_list[:i] + result_list[i + bll + 1:]
            break_check = 1
            break
    if break_check == 1:
        find_mine(result_list, len(result_list))
    else:
        print(result_list)

find_mine(given_list, gll)
