import sys
N = list(map(int, sys.stdin.readline().split()))

piList = []
element = [0] * N[1]
def ㅠ(left_repeat):
    global piList
    global element
    if left_repeat != 0:
        for i in range(1, N[0]+1):
            element[N[1]-left_repeat] = i
            ㅠ(left_repeat - 1)
    else:
        piList.append(element)





ㅠ(N[1])
print(piList)
print(N[0]**2)
