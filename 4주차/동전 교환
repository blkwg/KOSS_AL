import sys
N = int(sys.stdin.readline())
type = list(map(int, sys.stdin.readline().split()))
M = int(sys.stdin.readline())
count = 0
minimum_count = 500

def CET(left_amount, counter):
    global minimum_count
    if left_amount > 0:
        for i in range(N):
            CET(left_amount - i, counter + 1)
    elif left_amount == 0:
        if minimum_count > counter:
            minimum_count = counter


CET(M, count)
print(minimum_count)
