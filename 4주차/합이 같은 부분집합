import sys
N = int(sys.stdin.readline())
elements = list(map(int, sys.stdin.readline().split()))
add = sum(elements)

def make_half(x, y):
    if x <= N:
        if y == wanna_sum:
            print("YES")
            sys.exit(0)
        else:
            make_half(x+1, y + elements[x-1])
            make_half(x+1, y)
    else:
        print('', end='')







wanna_sum = add//2
check_number = 0
if add%2 != 1:
    make_half(1, check_number)
    print("NO")
else:
    print("NO")
